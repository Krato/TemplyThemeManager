{
    "name": "infinety-es/temply-theme-manager",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Infinety\\TemplyThemeManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Infinety\\TemplyThemeManager\\ThemeManagerToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
